@model Lab8.Models.Athlete

    <link rel="stylesheet" href="~/Content/bootstrap.css" />
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Create</title>
</head>
<body id="homeBody">
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-horizontal">
            <h4>Athlete</h4>
            <hr />


            <div class="form-group">
                @Html.LabelFor(model => model.TID, "TID", htmlAttributes: new { @class = "control-label col-md-2" })
                <div class="col-md-10">
                    @Html.DropDownList("TID", null, htmlAttributes: new { @class = "form-control" })
                    @Html.ValidationMessageFor(model => model.TID, "", new { @class = "text-danger" })
                </div>
            </div>

            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input type="submit" value="Create" class="btn btn-default" />
                </div>
            </div>
        </div>
    }

<div>
    @Html.ActionLink("Back to List", "Index", "graph",null, new { @class = "btn btn-primary btn-large" })
   
    @Html.ActionLink("Home", "Index", "Home",null, new { @class = "btn btn-primary btn-large" })
</div>

    
</body>
</html>
