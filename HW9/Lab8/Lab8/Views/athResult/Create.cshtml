@model Lab8.Models.Result

    <link rel="stylesheet" href="~/Content/bootstrap.css" />

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Create</title>
</head>
<body id="homeBody">
    @using (Html.BeginForm()) 
    {
        @Html.AntiForgeryToken()
        
        <div class="form-horizontal">
            <h4>Result</h4>
            <hr />
           
    
            <div class="form-group">
                @Html.LabelFor(model => model.AID, "AID", htmlAttributes: new { @class = "control-label col-md-2" })
                <div class="col-md-10">
                    @Html.DropDownList("AID", null, htmlAttributes: new { @class = "form-control" })
                    @Html.ValidationMessageFor(model => model.AID, "", new { @class = "text-danger" })
                </div>
            </div>
    
            
    
            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input type="submit" value="View Results" class="btn btn-default" />
                </div>
            </div>
        </div>
    }
    
<div>
    @Html.ActionLink("Back to List", "Index", "athResult", null, new { @class = "btn btn-primary btn-large"} )
    @Html.ActionLink("Home", "Index", "Home", null, new { @class = "btn btn-primary btn-large" })
</div>
</body>
</html>
